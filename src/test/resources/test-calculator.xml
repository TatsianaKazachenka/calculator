<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="Test calculator" parallel="classes">
    <test name="test difference">
        <classes>
            <class name="CalculatorTest"/>
            <parameter name="number1" value="18.0"></parameter>
            <parameter name="number2" value="8.0"></parameter>
        </classes>
    </test>
    <test name="test Discriminant">
        <classes>
            <class name="DiscriminantTest"/>
            <parameter name="a" value="2.0"></parameter>
            <parameter name="b" value="3.0"></parameter>
            <parameter name="c" value="4.0"></parameter>
        </classes>
    </test>
</suite>
